public Integer fibonacci(Integer n) {
    Integer start = 1;
    List<Integer> fibSequence = new List<Integer>();

    // 1
    // 1 + 0 = 1
    // 1 + 1 = 2
    // 2 + 1 = 3
    // 3 + 2 = 5
    // 5 + 3 = 8
    // 8 + 5 = 13
    fibSequence.add(0);
    fibSequence.add(1);

    for(Integer i=1; i<=n; i++){
        fibSequence.add(fibSequence[i-1] + fibSequence[i]);
    }

    System.debug(fibSequence);
    return(fibSequence[n]);
}